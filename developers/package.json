{
  "name": "potential-crud-root",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsnd --transpile-only --respawn --ignore-watch node_modules -r dotenv/config src/index.ts",
    "build": "rm -rf dist && tsc",
    "test": "jest --setupFiles ./setupTests.ts --dotenv_config_path=/.env.test --runInBand --verbose",
    "knex": "tsnd -r dotenv/config node_modules/.bin/knex --knexfile src/db/knexfile.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/express": "^4.17.8",
    "@types/faker": "^5.1.2",
    "@types/jest": "^26.0.14",
    "@types/nanoid": "^2.1.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^5.1.0",
    "husky": "^4.3.0",
    "jest": "^26.5.3",
    "lerna": "^3.22.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.21.6",
    "mysql": "^2.18.1",
    "nanoid": "^3.1.12",
    "shallow-equal-object": "^1.1.1",
    "supertest": "^5.0.0",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
